	<queryString>
		<![CDATA[WITH pagos_info AS (
    SELECT
        pagos.uid_actividad,
        pagos.id_clie_albaran,
        MAX(med.desmedpag_ven) AS metodo_pago
    FROM d_clie_albaranes_pag_tbl pagos
    JOIN d_medios_pago_ven_tbl med
        ON med.uid_actividad = pagos.uid_actividad
        AND med.id_medpag_ven = pagos.id_medpag_ven
    WHERE pagos.uid_actividad = $P{UID_ACTIVIDAD}
    GROUP BY pagos.uid_actividad, pagos.id_clie_albaran
)
SELECT
    dev.uid_actividad,
    dev.id_clie_albaran,
    dev.linea,
    CAST(alb_cab.fecha AS DATE) AS FECHA_DEVOLUCION,
    alb_cab.hora AS HORA_DEVOLUCION,
    dev.id_usuario_supervisor,
    usu.desusuario AS des_usuario_supervisor,
    art.codart AS CODART,
    art.desart AS ARTICULO_DEVUELTO,
    alb_det.cantidad AS CANTIDAD_DEVUELTA,
    motivos.descripcion AS MOTIVO_DEVOLUCION,
    alb_det.importe_total AS IMPORTE_DEVOLUCION,
    pagos_info.metodo_pago AS METODO_PAGO,
    almacenes.codalm AS CODALM,
    almacenes.desalm AS des_alm,
    alb_cab.referencia_cliente AS REFERENCIA,
    alb_cab.cod_albaran_origen AS REFERENCIA_VENTA,
    dev.validado AS VALIDADO,
    usu2.desusuario AS SUPERVISOR,
    dev.fecha_validacion AS FECHA_VALIDACION,
    dev.uid_ticket,
    alb_cab.usuario AS CAJERO,
    usu2.usuario AS VALIDADOR
FROM x_clie_albaranes_dev_tbl dev
JOIN d_clie_albaranes_det_tbl alb_det
    ON dev.uid_actividad = alb_det.uid_actividad
    AND dev.id_clie_albaran = alb_det.id_clie_albaran
    AND dev.linea = alb_det.linea
JOIN d_clie_albaranes_cab_tbl alb_cab
    ON dev.uid_actividad = alb_cab.uid_actividad
    AND dev.id_clie_albaran = alb_cab.id_clie_albaran
JOIN x_clie_albaranes_det_tbl xalb_det
    ON xalb_det.uid_actividad = alb_det.uid_actividad
    AND xalb_det.id_clie_albaran = alb_det.id_clie_albaran
    AND xalb_det.linea = alb_det.linea
LEFT JOIN x_motivos_tbl motivos
    ON motivos.uid_actividad = dev.uid_actividad
    AND motivos.codigo = xalb_det.codigo
LEFT JOIN d_usuarios_tbl usu
    ON dev.uid_actividad = usu.uid_instancia
    AND dev.id_usuario_supervisor = usu.id_usuario
LEFT JOIN d_usuarios_tbl usu2
    ON dev.uid_actividad = usu2.uid_instancia
    AND dev.id_usuario_validador = usu2.id_usuario
LEFT JOIN d_articulos_tbl art
    ON art.uid_actividad = alb_det.uid_actividad
    AND alb_det.codart = art.codart
LEFT JOIN d_almacenes_tbl almacenes
    ON almacenes.uid_actividad = alb_cab.uid_actividad
    AND almacenes.codalm = alb_cab.codalm
LEFT JOIN pagos_info
    ON pagos_info.uid_actividad = dev.uid_actividad
    AND pagos_info.id_clie_albaran = dev.id_clie_albaran
WHERE
    alb_cab.uid_actividad = $P{UID_ACTIVIDAD}
    AND alb_cab.fecha BETWEEN $P{FECHA_DESDE} AND $P{FECHA_HASTA}
    AND ($P{COD_TIENDA} IS NULL OR alb_cab.codalm = $P{COD_TIENDA})
    AND ($P{ID_USUARIO_SUPERVISOR} IS NULL OR dev.id_usuario_supervisor = $P{ID_USUARIO_SUPERVISOR})
    AND ($P{COD_ARTICULO} IS NULL OR alb_det.codart = $P{COD_ARTICULO})
    AND ($P{REFERENCIA_FACTURA} IS NULL OR alb_cab.referencia_cliente = $P{REFERENCIA_FACTURA})
    AND ($P{VALIDADO} IS NULL OR dev.validado = $P{VALIDADO})
    group by alb_cab.id_clie_albaran
ORDER BY alb_cab.fecha DESC, alb_cab.hora DESC, alb_cab.codalm desc, alb_cab.referencia_cliente DESC, art.desart DESC]]>
	</queryString>
