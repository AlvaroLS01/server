	<queryString>
		<![CDATA[WITH
tickets_unicos AS (
    SELECT DISTINCT uid_ticket, id_fidelizado, codalm, valor_cobrado
    FROM x_informe_promociones_tbl
    WHERE UID_ACTIVIDAD = $P{UID_ACTIVIDAD}
      AND ($P{FECHA_DESDE} IS NULL OR FECHA >= $P{FECHA_DESDE})
      AND ($P{FECHA_HASTA} IS NULL OR FECHA <= $P{FECHA_HASTA})
      AND ($P{COD_TIENDA_DESDE} IS NULL OR CODALM >= $P{COD_TIENDA_DESDE})
      AND ($P{COD_TIENDA_HASTA} IS NULL OR CODALM <= $P{COD_TIENDA_HASTA})
      AND ($P{COD_PROMOCION} IS NULL OR ID_PROMOCION = $P{COD_PROMOCION})
      AND ($P{COD_CAMPANIA} IS NULL OR COD_CAMPANIA = $P{COD_CAMPANIA})
),
total_por_fidelizado AS (
    SELECT
        id_fidelizado,
        codalm,
        SUM(valor_cobrado) AS total_valor_cobrado
    FROM tickets_unicos
    GROUP BY id_fidelizado, codalm
),
resumen_valores AS (
    SELECT
        id_fidelizado,
        id_promocion,
        codalm,
        SUM(valor_cobrado) AS valor_por_promocion
    FROM (
        SELECT DISTINCT uid_ticket, id_fidelizado, id_promocion, codalm, valor_cobrado
        FROM x_informe_promociones_tbl
        WHERE UID_ACTIVIDAD = $P{UID_ACTIVIDAD}
          AND ($P{FECHA_DESDE} IS NULL OR FECHA >= $P{FECHA_DESDE})
          AND ($P{FECHA_HASTA} IS NULL OR FECHA <= $P{FECHA_HASTA})
          AND ($P{COD_TIENDA_DESDE} IS NULL OR CODALM >= $P{COD_TIENDA_DESDE})
          AND ($P{COD_TIENDA_HASTA} IS NULL OR CODALM <= $P{COD_TIENDA_HASTA})
          AND ($P{COD_PROMOCION} IS NULL OR ID_PROMOCION = $P{COD_PROMOCION})
          AND ($P{COD_CAMPANIA} IS NULL OR COD_CAMPANIA = $P{COD_CAMPANIA})
    ) datos_filtrados
    GROUP BY id_fidelizado, id_promocion, codalm
),
colectivos AS (
    SELECT id_fidelizado, GROUP_CONCAT(DISTINCT cod_colectivo) AS colectivos
    FROM f_fidelizados_colectivos_tbl
    WHERE uid_instancia = $P{UID_ACTIVIDAD}
    GROUP BY id_fidelizado
)
SELECT
    colectivos.colectivos,
    x.UID_TICKET,
    x.DESCRIPCION_PROMOCION,
    x.ID_FIDELIZADO,
    CONCAT(x.CODALM, ' - ', x.DESALM) AS TIENDA,
    x.NOMBRE_FIDELIZADO AS NOMBRE,
    IFNULL(r.valor_por_promocion, 0) AS VALOR_COBRADO,
    SUM(x.VALOR_DESCUENTO) AS IMPORTE_DESCUENTO,
    DATE(x.FECHA) AS FECHA,
    ROUND(
        (SUM(x.VALOR_DESCUENTO) /
        NULLIF(tp.total_valor_cobrado - SUM(x.VALOR_DESCUENTO), 0)) * 100, 2
    ) AS PORC_DESCUENTO_APLICADO,
    IFNULL(tp.total_valor_cobrado, 0) AS TOTAL
FROM x_informe_promociones_tbl x
LEFT JOIN f_fidelizados_tbl fid
    ON x.UID_ACTIVIDAD = fid.uid_instancia
   AND x.ID_FIDELIZADO = fid.id_fidelizado
LEFT JOIN colectivos ON colectivos.id_fidelizado = x.ID_FIDELIZADO
LEFT JOIN resumen_valores r
    ON r.id_fidelizado <=> x.ID_FIDELIZADO
   AND r.id_promocion = x.ID_PROMOCION
   AND r.codalm = x.CODALM
LEFT JOIN total_por_fidelizado tp
    ON tp.id_fidelizado <=> x.ID_FIDELIZADO
   AND tp.codalm = x.CODALM
WHERE
    x.UID_ACTIVIDAD = $P{UID_ACTIVIDAD}
  AND ($P{FECHA_DESDE} IS NULL OR x.FECHA >= $P{FECHA_DESDE})
  AND ($P{FECHA_HASTA} IS NULL OR x.FECHA <= $P{FECHA_HASTA})
  AND ($P{COD_TIENDA_DESDE} IS NULL OR x.CODALM >= $P{COD_TIENDA_DESDE})
  AND ($P{COD_TIENDA_HASTA} IS NULL OR x.CODALM <= $P{COD_TIENDA_HASTA})
  AND ($P{COD_PROMOCION} IS NULL OR x.ID_PROMOCION = $P{COD_PROMOCION})
  AND ($P{COD_CAMPANIA} IS NULL OR x.COD_CAMPANIA = $P{COD_CAMPANIA})
  AND ($P{DOCUMENTO} IS NULL OR fid.documento = $P{DOCUMENTO})
  AND ($P{COD_COLECTIVO} IS NULL OR colectivos.colectivos IS NOT NULL)
GROUP BY
    x.ID_FIDELIZADO,
    x.ID_PROMOCION,
    x.CODALM
ORDER BY x.ID_FIDELIZADO DESC, x.ID_PROMOCION DESC;]]>
