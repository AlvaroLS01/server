	</parameter>
	<parameter name="COD_TIENDA_HASTA" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["ZZZZZ"]]></defaultValueExpression>
	</parameter>
	<parameter name="CAJERO" class="java.lang.String" isForPrompting="false"/>
	<parameter name="UID_ACTIVIDAD" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[WITH cab_filtro AS (
  SELECT
      det.UID_ACTIVIDAD,
      det.UID_DIARIO_CAJA,
      cab.CODALM,
      cab.CODCAJA,
      usr_cab.ID_USUARIO   AS ID_USUARIO,
      cab.USUARIO          AS USUARIO_CAB,
      det.CODMEDPAG,
      det.CODCONCEPTO_MOV,
      det.CARGO,
      det.ABONO,
      det.USUARIO          AS USUARIO_DET
  FROM  D_CAJA_CAB_TBL  cab
  LEFT  JOIN D_USUARIOS_TBL usr_cab
         ON usr_cab.UID_INSTANCIA = cab.UID_ACTIVIDAD
        AND usr_cab.USUARIO       = cab.USUARIO
  JOIN  D_CAJA_DET_TBL  det
         ON det.UID_ACTIVIDAD   = cab.UID_ACTIVIDAD
        AND det.UID_DIARIO_CAJA = cab.UID_DIARIO_CAJA
  WHERE cab.UID_ACTIVIDAD = $P{UID_ACTIVIDAD}
    AND ( $P{COD_TIENDA_DESDE} IS NULL OR cab.CODALM >= $P{COD_TIENDA_DESDE} )
    AND ( $P{COD_TIENDA_HASTA} IS NULL OR cab.CODALM <= $P{COD_TIENDA_HASTA} )
    AND ( $P{CAJERO} IS NULL OR usr_cab.ID_USUARIO = $P{CAJERO} )
    AND det.FECHA BETWEEN
        TIMESTAMP(CONCAT(DATE($P{FECHA_DESDE}), ' 00:00:00'))
    AND TIMESTAMP(CONCAT(DATE($P{FECHA_HASTA}),  ' 23:59:59'))
)
SELECT
    SUM(IF(cf.CODCONCEPTO_MOV IS NULL, 1, 0)) AS VECES,
    cf.CODMEDPAG,
    med.DESMEDPAG,
    SUM( CASE WHEN cf.CODCONCEPTO_MOV IS NULL
              THEN cf.CARGO - cf.ABONO ELSE 0 END )  AS IMPORTE,
    cf.CODALM,
    alm.DESALM,
    IF($P{CAJERO} IS NULL, 'TODOS', usr_det.DESUSUARIO)  AS NOMBRE_CAJERO,
    SUM( CASE WHEN cf.CODCONCEPTO_MOV IN ('00','01','80','85')
               AND cf.CODCAJA NOT IN ('80','90')
               THEN cf.CARGO END )                     AS APORTACION,
    SUM( CASE WHEN cf.CODCONCEPTO_MOV IN ('02','03')
               THEN cf.CARGO - cf.ABONO ELSE 0 END )          AS RETIRADA,
    SUM( CASE WHEN cf.CODCONCEPTO_MOV IN ('81','85')
               AND cf.CODCAJA = '80'
               THEN cf.CARGO END )                     AS DESAPORTACION
FROM cab_filtro cf
LEFT JOIN D_MEDIOS_PAGO_TBL med
       ON med.UID_ACTIVIDAD = cf.UID_ACTIVIDAD
      AND med.CODMEDPAG     = cf.CODMEDPAG
LEFT JOIN D_ALMACENES_TBL alm
       ON alm.UID_ACTIVIDAD = cf.UID_ACTIVIDAD
      AND alm.CODALM        = cf.CODALM
LEFT JOIN D_USUARIOS_TBL usr_det
       ON usr_det.UID_INSTANCIA = cf.UID_ACTIVIDAD
      AND usr_det.USUARIO       = cf.USUARIO_DET
GROUP BY
    cf.CODALM,
    cf.CODMEDPAG,
    med.DESMEDPAG,
    alm.DESALM,
    IF($P{CAJERO} IS NULL, 'TODOS', usr_det.DESUSUARIO)
ORDER BY cf.CODALM;]]>
	</queryString>
