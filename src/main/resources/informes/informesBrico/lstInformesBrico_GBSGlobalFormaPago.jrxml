	<queryString>
		<![CDATA[SELECT
 PAG.LINEA,
 PAG.ID_CLIE_ALBARAN,
 DOCU.ID_TIPO_DOCUMENTO,
 DOCU.CODTIPODOCUMENTO,
 DOCU.DESTIPODOCUMENTO,
 CAB.NUMALB,
 CMOV.DOCUMENTO,
 VEN.ID_MEDPAG_VEN,
 MPAG.CODMEDPAG,
 MPAG.DESMEDPAG,
 TARJ.CODTIPOTARJ,
 TIPOTARJ.DESTIPOTARJ,
 CMOV.ID_CUENTA_TARJETA,
 TARJ.NUMERO_TARJETA, DATE(CAB.FECHA) AS FECHA, DATE(CMOV.FECHA) AS FECHA_CMOV, DATE(TARJ.FECHA_EMISION) AS FECHA_EMISION, DATE(TARJ.FECHA_ULTIMO_USO) AS FECHA_ULTIMO_USO, DATE(CMOV.FECHA_PROCESO) AS FECHA_PROCESO,
 CMOV.ENTRADA,
 CMOV.SALIDA,
 CUEN.SALDO,
 ALM.CODALM,
 ALM.DESALM,
 CAB.TOTAL,
 PAG.IMPORTE, CASE WHEN DOCU.CODPAIS = "PT" THEN CASE WHEN CAB.PERIODO = '2022' THEN
 SUBSTR(CMOV.CONCEPTO, INSTR(CMOV.CONCEPTO, "/")-13, LENGTH(CMOV.CONCEPTO)) ELSE
 SUBSTR(CMOV.CONCEPTO, INSTR(CMOV.CONCEPTO, "/")-9, LENGTH(CMOV.CONCEPTO)) END WHEN DOCU.CODPAIS = "ES" THEN
 SUBSTR(CMOV.CONCEPTO, INSTR(CMOV.CONCEPTO, "/")-13, LENGTH(CMOV.CONCEPTO)) END CONCEPTO,
 CAB.REFERENCIA_CLIENTE,
 CAB.USUARIO,
 XPAG.NUM_PEDIDO,
 XPAG.NUM_FINANCIACION
FROM
 D_CLIE_ALBARANES_CAB_TBL CAB
LEFT JOIN
 D_CLIE_ALBARANES_PAG_TBL PAG ON (PAG.UID_ACTIVIDAD = CAB.UID_ACTIVIDAD AND
 PAG.ID_CLIE_ALBARAN = CAB.ID_CLIE_ALBARAN)
LEFT JOIN
 X_CUENTAS_MOVIMIENTOS_TBL XMOV ON (XMOV.UID_ACTIVIDAD = CAB.UID_ACTIVIDAD AND
 XMOV.ID_CLIE_ALBARAN = CAB.ID_CLIE_ALBARAN AND
 XMOV.LINEA = PAG.LINEA)
LEFT JOIN
 F_CUENTAS_MOVIMIENTOS_TBL CMOV ON (CMOV.UID_INSTANCIA = XMOV.UID_INSTANCIA AND
 CMOV.UID_TRANSACCION = XMOV.UID_TRANSACCION)
LEFT JOIN
 D_MEDIOS_PAGO_VEN_TBL VEN ON (VEN.UID_ACTIVIDAD = PAG.UID_ACTIVIDAD AND
 VEN.ID_MEDPAG_VEN = PAG.ID_MEDPAG_VEN)
LEFT JOIN
 D_MEDIOS_PAGO_TBL MPAG ON (MPAG.UID_ACTIVIDAD = CAB.UID_ACTIVIDAD AND
 MPAG.CODMEDPAG = VEN.CODMEDPAG)
LEFT JOIN
 D_TIPOS_DOCUMENTOS_TBL DOCU ON (DOCU.UID_ACTIVIDAD = CAB.UID_ACTIVIDAD AND
 DOCU.ID_TIPO_DOCUMENTO = CAB.ID_TIPO_DOCUMENTO)
LEFT JOIN
 F_TARJETAS_TBL TARJ ON (TARJ.UID_INSTANCIA = CMOV.UID_INSTANCIA AND
 TARJ.ID_TARJETA = CMOV.ID_TARJETA AND
 TARJ.ID_CUENTA_TARJETA = CMOV.ID_CUENTA_TARJETA)
LEFT JOIN
 F_TARJETAS_TIPOS_TBL TIPOTARJ ON (TIPOTARJ.CODTIPOTARJ = TARJ.CODTIPOTARJ AND
 TIPOTARJ.uid_instancia = CMOV.uid_instancia)
LEFT JOIN
 F_CUENTAS_TBL CUEN ON (CUEN.UID_INSTANCIA = CMOV.UID_INSTANCIA AND
 CUEN.ID_CUENTA_TARJETA = CMOV.ID_CUENTA_TARJETA)
LEFT JOIN
 D_ALMACENES_TBL ALM ON (ALM.UID_ACTIVIDAD = CAB.UID_ACTIVIDAD AND
 ALM.CODALM = CAB.CODALM)
LEFT JOIN
 X_CLIE_ALBARANES_PAG_TBL XPAG ON (XPAG.UID_ACTIVIDAD = CAB.UID_ACTIVIDAD AND
 XPAG.ID_CLIE_ALBARAN = CAB.ID_CLIE_ALBARAN AND
 XPAG.LINEA = PAG.LINEA)
WHERE
 CAB.UID_ACTIVIDAD = $P{UID_ACTIVIDAD} AND
 MPAG.CODMEDPAG <> 0000 AND
 MPAG.CODMEDPAG <> 0010 AND
 MPAG.CODMEDPAG <> 0011 AND
 ($P{COD_FORMA_PAGO} IS NULL OR $P{COD_FORMA_PAGO} IS NOT NULL AND MPAG.CODMEDPAG = $P{COD_FORMA_PAGO}) AND
 ($P{COD_TIPO_TARJETA} IS NULL OR $P{COD_TIPO_TARJETA} IS NOT NULL AND TARJ.CODTIPOTARJ = $P{COD_TIPO_TARJETA}) AND
 (CAB.FECHA >= $P{FECHA_DESDE} AND CAB.FECHA <= $P{FECHA_HASTA}) AND
 ($P{COD_TIENDA_DESDE} IS NULL OR ($P{COD_TIENDA_DESDE} IS NOT NULL AND CAB.CODALM >= $P{COD_TIENDA_DESDE})) AND
 ($P{COD_TIENDA_HASTA} IS NULL OR ($P{COD_TIENDA_HASTA} IS NOT NULL AND CAB.CODALM <= $P{COD_TIENDA_HASTA})) AND
 ($P{CAJERO} IS NULL OR ($P{CAJERO} IS NOT NULL AND CAB.CODCAJA = $P{CAJERO})) AND
 ($P{FECHA_RECEPCION_EMISION_TARJETA} IS NULL OR ($P{FECHA_RECEPCION_EMISION_TARJETA} IS NOT NULL AND DATE(TARJ.FECHA_EMISION) >= $P{FECHA_RECEPCION_EMISION_TARJETA})) AND
 ($P{FECHA_EMISION_USO_TARJETA} IS NULL OR ($P{FECHA_EMISION_USO_TARJETA} IS NOT NULL AND DATE(TARJ.FECHA_ULTIMO_USO) >= $P{FECHA_EMISION_USO_TARJETA}))
ORDER BY
 MPAG.DESMEDPAG,
 CAB.FECHA,
 CMOV.FECHA_PROCESO,
 TIPOTARJ.DESTIPOTARJ]]>
	</queryString>
