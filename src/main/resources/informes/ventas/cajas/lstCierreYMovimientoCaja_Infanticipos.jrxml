	<queryString>
		<![CDATA[SELECT
  XAN.ID_CLIE_ALBARAN,
  XAN.NUM_ANTICIPO,
  XAN.IMPORTE AS IMPORTE_PAGADO ,
  IF(XAN.ESTADO IN ('LIQUIDADO','DEVUELTO'), -XAN.IMPORTE,'') AS IMPORTE_LIQUIDADO,
  XAN.UID_TICKET ,
  XAN.ESTADO ,
  B.FECHA AS FECHA_ANTICIPO,
  B.REFERENCIA_CLIENTE ,
  C.FECHA AS FECHA_LIQUIDACION,
  ALM.CODALM,
  ALM.DESALM
FROM X_ANTICIPOS_TBL XAN LEFT JOIN
     D_CLIE_ALBARANES_CAB_TBL AS B ON (XAN.UID_ACTIVIDAD = B.uid_actividad AND XAN.ID_CLIE_ALBARAN=B.ID_CLIE_ALBARAN) LEFT JOIN
     D_CLIE_ALBARANES_CAB_TBL AS C ON (XAN.UID_TICKET=C.UID_TICKET) LEFT JOIN
     D_ALMACENES_TBL ALM ON (XAN.UID_ACTIVIDAD = ALM.uid_actividad AND ALM.CODALM = B.CODALM)
WHERE B.FECHA BETWEEN $P{FECHA_DESDE} AND $P{FECHA_HASTA}
  AND XAN.UID_ACTIVIDAD = $P{UID_ACTIVIDAD}
  AND B.CODALM = $P{CODALMACEN_DESDE}
  AND (XAN.NUM_ANTICIPO = $P{NUM_ANTICIPO} OR $P{NUM_ANTICIPO} IS NULL)
  AND (XAN.ESTADO = $P{ESTADO} OR $P{ESTADO} IS NULL)
ORDER BY B.FECHA DESC;]]>
	</queryString>
