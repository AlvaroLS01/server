	<parameter name="COD_TIENDA_HASTA" class="java.lang.String"/>
	<parameter name="CAJERO" class="java.lang.String" isForPrompting="false"/>
	<parameter name="DES_TIENDA_DESDE" class="java.lang.String" isForPrompting="false"/>
	<parameter name="DES_TIENDA_HASTA" class="java.lang.String" isForPrompting="false"/>
