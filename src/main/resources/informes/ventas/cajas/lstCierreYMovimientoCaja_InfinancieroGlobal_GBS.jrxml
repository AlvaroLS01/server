	<queryString>
		<![CDATA[SELECT CAJA.VECES,
       CAJA.CODMEDPAG,
       CAJA.DESMEDPAG,
       CAJA.IMPORTE,
       CAJA.CODALM,
       CAJA.DESALM,
  (SELECT (SUM(DET.CARGO)) AS IMPORTE
   FROM D_CAJA_DET_TBL DET INNER JOIN
        D_CAJA_CAB_TBL CAB ON (CAB.UID_DIARIO_CAJA=DET.UID_DIARIO_CAJA) LEFT JOIN
        D_MEDIOS_PAGO_TBL MED ON (MED.UID_ACTIVIDAD = DET.UID_ACTIVIDAD AND
                                  MED.CODMEDPAG     = DET.CODMEDPAG)
   WHERE DET.FECHA BETWEEN $P{FECHA_DESDE} AND $P{FECHA_HASTA}
     AND (DET.CODCONCEPTO_MOV = '00'
          OR DET.CODCONCEPTO_MOV = '01'
          OR DET.CODCONCEPTO_MOV = '80')
     AND DET.CODMEDPAG = CAJA.CODMEDPAG
     AND CAB.CODCAJA !='80'
     AND CAB.CODCAJA!='90'
     AND (CODALM >= $P{COD_TIENDA_DESDE}
          AND CODALM <= $P{COD_TIENDA_HASTA})
   GROUP BY DET.CODMEDPAG,
            MED.DESMEDPAG,
            CODALM,
            DESALM) AS APORTACIÓN,
       IFNULL(
                (SELECT (SUM(DET.CARGO) - SUM(DET.ABONO)) AS IMPORTE
                 FROM D_CAJA_DET_TBL DET INNER JOIN
                      D_CAJA_CAB_TBL CAB ON (CAB.UID_DIARIO_CAJA = DET.UID_DIARIO_CAJA) LEFT JOIN
                      D_MEDIOS_PAGO_TBL MED ON (MED.UID_ACTIVIDAD = DET.UID_ACTIVIDAD AND
                                                MED.CODMEDPAG     = DET.CODMEDPAG)
                 WHERE DET.FECHA BETWEEN $P{FECHA_DESDE} AND $P{FECHA_HASTA}
                   AND (CODALM >= $P{COD_TIENDA_DESDE}
                        AND CODALM <= $P{COD_TIENDA_HASTA})
                   AND (DET.CODCONCEPTO_MOV = '02'
                        OR DET.CODCONCEPTO_MOV = '03')
                   AND DET.CODMEDPAG = CAJA.CODMEDPAG
                 GROUP BY DET.CODMEDPAG, MED.DESMEDPAG, CODALM, DESALM),0) AS RETIRADA,

  (SELECT SUM(CARGO)
   FROM D_CAJA_DET_TBL DET INNER JOIN
        D_CAJA_CAB_TBL CAB ON (CAB.UID_DIARIO_CAJA = DET.UID_DIARIO_CAJA)
   WHERE CODCONCEPTO_MOV = '81'
     AND FECHA BETWEEN $P{FECHA_DESDE} AND $P{FECHA_HASTA}
     AND (CODALM >= $P{COD_TIENDA_DESDE}
          AND CODALM <= $P{COD_TIENDA_HASTA})
     AND DET.CODMEDPAG = CAJA.CODMEDPAG) AS DESAPORTACION
FROM
  (SELECT (COUNT(*)) AS VECES,
          DET.CODMEDPAG,
          MED.DESMEDPAG,
          (SUM(DET.CARGO) - SUM(DET.ABONO)) AS IMPORTE,
          CAB.CODALM,
          ALM.DESALM
   FROM D_CAJA_DET_TBL DET LEFT JOIN
        D_CAJA_CAB_TBL CAB ON (CAB.UID_ACTIVIDAD   = DET.UID_ACTIVIDAD AND
                               CAB.UID_DIARIO_CAJA = DET.UID_DIARIO_CAJA AND
                              ($P{CAJERO} IS NULL OR $P{CAJERO} IS NOT NULL AND CAB.CODCAJA = $P{CAJERO})) LEFT JOIN
        D_MEDIOS_PAGO_TBL MED ON (MED.UID_ACTIVIDAD = DET.UID_ACTIVIDAD AND
                                  MED.CODMEDPAG     = DET.CODMEDPAG) LEFT JOIN
        D_ALMACENES_TBL ALM ON (ALM.UID_ACTIVIDAD = CAB.UID_ACTIVIDAD AND
                                ALM.CODALM        = CAB.CODALM)
   WHERE DET.FECHA BETWEEN $P{FECHA_DESDE} AND $P{FECHA_HASTA}
     AND DET.CODCONCEPTO_MOV IS NULL
   GROUP BY DET.CODMEDPAG,
            MED.DESMEDPAG,
            CAB.CODALM,
            ALM.DESALM) CAJA
ORDER BY CAJA.DESMEDPAG]]>
