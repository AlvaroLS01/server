	<queryString>
		<![CDATA[select COUNT(*), SUM(DET.IMPORTE_TOTAL), DET.FECHA
from d_clie_albaranes_det_tbl DET LEFT JOIN
     d_clie_albaranes_cab_tbl CAB ON (CAB.uid_actividad = DET.uid_actividad AND
                                      CAB.id_clie_albaran = DET.id_clie_albaran)
where DET.FECHA BETWEEN $P{PFECHA_DESDE} AND $P{PFECHA_HASTA}
AND DET.UID_ACTIVIDAD = $P{UID_ACTIVIDAD}
group by DET.FECHA]]>
	</queryString>
	<field name="COUNT(*)" class="java.lang.Long"/>
	<field name="SUM(DET.IMPORTE_TOTAL)" class="java.math.BigDecimal"/>
	<field name="FECHA" class="java.sql.Timestamp"/>
