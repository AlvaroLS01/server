	<variable name="FECHAS" class="java.util.Date">
		<variableExpression><![CDATA[!$F{DESMEDPAG}.contains("TARJETA") ||
!$F{DESMEDPAG}.contains("tarjeta") ||
!$F{DESMEDPAG}.contains("Tarjeta") ?
$F{FECHA} :
($F{SALIDA}.doubleValue() >= 0.00 && $P{TIPO_TARJETA_FECHA}.equals("fechaRecepcion") ?
$F{FECHA_PROCESO} :
($F{SALIDA}.doubleValue() <= 0.00 && $P{TIPO_TARJETA_FECHA}.equals("fechaEmision") ?
$F{FECHA_PROCESO} :
$F{FECHA}))]]></variableExpression>
	</variable>
	<variable name="NUM_OPER_EMITIDAS" class="java.lang.Integer" resetType="Group" resetGroup="GR_FECHA" calculation="Count">
		<variableExpression><![CDATA[($F{CODTIPODOCUMENTO}.equals("FS") ||
$F{CODTIPODOCUMENTO}.equals("FT")) && $F{TOTAL}.doubleValue() > 0.00 ?
$F{CODTIPOTARJ} != null && $F{SALIDA}.doubleValue() > 0.00 ?
$F{ID_CUENTA_TARJETA} :
$F{ID_CLIE_ALBARAN} :
0.00]]></variableExpression>
	</variable>
	<variable name="NUM_OPER_RECIBIDAS" class="java.lang.Integer" resetType="Group" resetGroup="GR_FECHA" calculation="Count">
		<variableExpression><![CDATA[($F{CODTIPODOCUMENTO}.equals("FS") ||
$F{CODTIPODOCUMENTO}.equals("FT") ||
$F{CODTIPODOCUMENTO}.equals("NC")) && $F{TOTAL}.doubleValue() < 0.00 ?
$F{CODTIPOTARJ} != null && $F{ENTRADA}.doubleValue() > 0.00 ?
$F{ID_CUENTA_TARJETA} :
$F{ID_CLIE_ALBARAN} :
0.00]]></variableExpression>
	</variable>
	<variable name="IMPORTE_EMITIDO" class="java.math.BigDecimal" resetType="Group" resetGroup="GR_FECHA" calculation="Sum">
		<variableExpression><![CDATA[$F{IMPORTE}.doubleValue() >= 0.00 &&
($F{ID_TIPO_DOCUMENTO}.intValue() == 1 ||
$F{ID_TIPO_DOCUMENTO}.intValue() == 1001 ||
$F{ID_TIPO_DOCUMENTO}.intValue() == 2 ||
$F{ID_TIPO_DOCUMENTO}.intValue() == 1002 ) &&
$F{TOTAL}.doubleValue() >= 0.00  ?
($F{ID_CUENTA_TARJETA} != null ?
     $F{SALIDA} :
     $F{IMPORTE}) :
 BigDecimal.ZERO]]></variableExpression>
	</variable>
	<variable name="IMPORTE_RECIBIDO" class="java.math.BigDecimal" resetType="Group" resetGroup="GR_FECHA" calculation="Sum">
		<variableExpression><![CDATA[($F{IMPORTE}.doubleValue() < 0.00 &&
($F{ID_TIPO_DOCUMENTO}.intValue() == 1 || $F{ID_TIPO_DOCUMENTO}.intValue() == 1001 ||
$F{ID_TIPO_DOCUMENTO}.intValue() == 2 || $F{ID_TIPO_DOCUMENTO}.intValue() == 1002 ||
$F{ID_TIPO_DOCUMENTO}.intValue() == 11 )) ||

($F{ID_TIPO_DOCUMENTO}.intValue() == 3 || $F{ID_TIPO_DOCUMENTO}.intValue() == 1003  ) ?
($F{ID_CUENTA_TARJETA} != null ?
     $F{SALIDA} : $F{IMPORTE})
 : BigDecimal.ZERO]]></variableExpression>
	</variable>
	<variable name="TOTAL_DIFERENCIAS" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{CODMEDPAG}.equals("0020") ?
$V{IMPORTE_RECIBIDO}.subtract($V{IMPORTE_EMITIDO}.abs()) :
$V{IMPORTE_EMITIDO}.subtract($V{IMPORTE_RECIBIDO}.abs())]]></variableExpression>
	</variable>
	<variable name="SUMA_EMITIDO" class="java.math.BigDecimal" resetType="Group" resetGroup="GR_TIPOSPAGO" calculation="Sum">
		<variableExpression><![CDATA[$V{IMPORTE_EMITIDO}]]></variableExpression>
