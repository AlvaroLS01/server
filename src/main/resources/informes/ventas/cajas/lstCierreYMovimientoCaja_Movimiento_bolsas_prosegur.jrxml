	<queryString>
		<![CDATA[SELECT  MOV.UID_ACTIVIDAD,
	MOV.UID_DIARIO_CAJA,
	MOV.LINEA_91 AS BOLSA,
	MOV.LINEA_90 AS ESTUCHE,
	MOV.DOCUMENTO,
	MOV.FECHA_BOLSA,
	MOV.IMPORTE AS ABONO,
	MOV.USUARIO,
	DET_90.DOCUMENTO AS DOCUMENTO_ESTUCHE,
 	DET_90.FECHA AS FECHA_ESTUCHE,
 	DET_90.CARGO,
 	DET_90.USUARIO AS USUARIO_ESTUCHE
FROM
	X_MOVIMIENTOS_RETIRADAS_BANCO_TBL MOV INNER JOIN
	D_CAJA_DET_TBL DET_90 ON (DET_90.UID_ACTIVIDAD = MOV.UID_ACTIVIDAD AND DET_90.UID_DIARIO_CAJA = MOV.UID_DIARIO_CAJA AND DET_90.LINEA = MOV.LINEA_90) INNER JOIN
	D_CAJA_CAB_TBL CAB ON (CAB.UID_ACTIVIDAD = DET_90.UID_ACTIVIDAD AND CAB.UID_DIARIO_CAJA = DET_90.UID_DIARIO_CAJA)
WHERE MOV.FECHA_BOLSA BETWEEN $P{FECHA_DESDE} AND $P{FECHA_HASTA} AND
 CAB.CODALM = $P{CODALMACEN_DESDE} AND MOV.UID_ACTIVIDAD = $P{UID_ACTIVIDAD}
 and DET_90.codconcepto_mov in (90,91)
ORDER BY DET_90.FECHA DESC]]>
	</queryString>
