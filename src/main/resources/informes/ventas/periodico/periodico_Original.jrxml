	<queryString>
		<![CDATA[SELECT  CAB.CODALM, CAB.DESALM, CAB.CODCAJA, CAB.FECHA, CAB.HORA,
        DOC.CODTIPODOCUMENTO, CAB.SERIE_ALBARAN, CAB.PERIODO, CAB.NUMALB,
        CAB.CODCLI, CAB.DESCLI, DET.CODART, DET.DESART, DET.DESGLOSE1, DESGLOSE2,
        DET.CANTIDAD, DET.PRECIO, DET.IMPORTE, DET.PRECIO, DET.IMPORTE_TOTAL, CAB.BASE, CAB.IMPUESTOS, CAB.TOTAL

FROM D_CLIE_ALBARANES_CAB CAB
INNER JOIN D_CLIE_ALBARANES_DET DET
ON CAB.UID_ACTIVIDAD = DET.UID_ACTIVIDAD
AND CAB.ID_CLIE_ALBARAN = DET.ID_CLIE_ALBARAN
INNER JOIN D_TIPOS_DOCUMENTOS_TBL DOC
ON DOC.UID_ACTIVIDAD = CAB.UID_ACTIVIDAD
AND DOC.ID_TIPO_DOCUMENTO = CAB.ID_TIPO_DOCUMENTO

WHERE  CAB.UID_ACTIVIDAD = $P{UID_ACTIVIDAD}
AND CAB.CODALM BETWEEN $P{PDESDE_CODALM} AND $P{PHASTA_CODALM}
AND CAB.CODCAJA BETWEEN $P{PDESDE_CODCAJA} AND $P{PHASTA_CODCAJA}
AND CAB.FECHA BETWEEN $P{PDESDE_FECHA} AND $P{PHASTA_FECHA}
AND DOC.CODTIPODOCUMENTO BETWEEN $P{PDESDE_TIPODOCUMENTO} AND $P{PHASTA_TIPODOCUMENTO}

ORDER BY CAB.CODALM, CAB.CODCAJA, CAB.FECHA, CAB.PERIODO, CAB.CODSERIE, CAB.NUMALB, DOC.CODTIPODOCUMENTO]]>
	</queryString>
