	<queryString>
		<![CDATA[SELECT
     CAB.UID_ACTIVIDAD,
     CAB.CODEMP,
     CAB.CODALM,
     CAB.FECHA,
     CAB.ID_CLIE_ALBARAN,
     SUM(DET.IMPORTE * (-1*CON.OPERACION_ALMACEN)) AS IMPORTE_TICKET,
     SUM(DET.IMPORTE_TOTAL * (-1*CON.OPERACION_ALMACEN)) AS IMPORTE_TOTAL_TICKET,
     ALMACENES.DESALM
FROM D_CLIE_ALBARANES_DET_TBL DET
	INNER JOIN D_CLIE_ALBARANES_CAB_TBL CAB ON DET.UID_ACTIVIDAD = CAB.UID_ACTIVIDAD AND DET.ID_CLIE_ALBARAN = CAB.ID_CLIE_ALBARAN
	INNER JOIN D_ARTICULOS_TBL ART ON DET.UID_ACTIVIDAD = ART.UID_ACTIVIDAD AND DET.CODART = ART.CODART
	 INNER JOIN D_ALMACENES_CONCEPTOS_TBL CON ON (CON.UID_ACTIVIDAD = CAB.UID_ACTIVIDAD AND CON.CODAPLICACION = CAB.CODAPLICACION AND CON.CODCONALM = CAB.CODCONALM)
INNER JOIN
     D_ALMACENES_TBL ALMACENES
ON (CAB.UID_ACTIVIDAD = ALMACENES.UID_ACTIVIDAD AND CAB.CODALM = ALMACENES.CODALM)
WHERE CAB.UID_ACTIVIDAD = $P{UID_ACTIVIDAD} AND
      CAB.CODEMP= $P{CODEMP} AND
      CAB.CODCLI >= $P{CODCLI_DESDE}
 AND  CAB.CODCLI <= $P{CODCLI_HASTA}
 AND  CAB.CODCONALM >= $P{CODCONALM_DESDE}
 AND  CAB.CODCONALM <= $P{CODCONALM_HASTA}
 AND  CAST(CAB.FECHA AS DATE) >= $P{FECHA_DESDE}
 AND  CAST(CAB.FECHA AS DATE) <= $P{FECHA_HASTA}
 AND  DET.CODART >= $P{CODART_DESDE}
 AND  DET.CODART <= $P{CODART_HASTA}
 AND  (ART.CODFAM >= $P{CODFAM_DESDE} AND ART.CODFAM <= $P{CODFAM_HASTA}
	  OR ('0' = $P{CODFAM_DESDE} AND 'ZZZZ' = $P{CODFAM_HASTA} AND ART.CODFAM IS NULL))
 AND  CAB.CODALM >= $P{CODALM_DESDE}
 AND  CAB.CODALM <= $P{CODALM_HASTA}
 AND  CAB.CODAPLICACION = 'VENT'
GROUP BY
      CAB.UID_ACTIVIDAD, CAB.CODEMP, CAB.CODALM, CAB.FECHA, CAB.ID_CLIE_ALBARAN, ALMACENES.DESALM
ORDER BY
     CAB.CODEMP ASC,
     CAB.CODALM ASC,
     CAB.FECHA]]>
	</queryString>
