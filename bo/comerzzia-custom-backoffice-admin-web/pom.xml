<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>com.comerzzia.bricodepot</groupId>
		<artifactId>comerzzia-custom-backoffice</artifactId>
		<version>${revision}</version>
		<relativePath>../</relativePath>
	</parent>
	<artifactId>comerzzia-custom-backoffice-admin-web</artifactId>
	<packaging>war</packaging>

	<dependencies>
		<dependency>
			<groupId>javax.servlet</groupId>
			<artifactId>javax.servlet-api</artifactId>
			<scope>provided</scope>
		</dependency>
		<!-- comerzzia web modules -->
		<dependency>
			<groupId>com.comerzzia</groupId>
			<artifactId>comerzzia-admin-ui</artifactId>
		</dependency>
		<!-- custom project services -->
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>comerzzia-custom-backoffice-services</artifactId>
			<version>${revision}</version>
		</dependency>
		<dependency>
			<groupId>com.comerzzia.api.v2.loyalty</groupId>
			<artifactId>comerzzia-loyalty-client-backoffice</artifactId>
			<scope>runtime</scope>
		</dependency>
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>comerzzia-bricodepot-pricechangetracker-client</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>	
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-deploy-plugin</artifactId>
				<configuration>
					<skip>true</skip>
				</configuration>
			</plugin>

		</plugins>
	</build>


</project>
