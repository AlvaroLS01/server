<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="lstSituacionVentas_Comprobante_ContadorMinMax" pageWidth="555" pageHeight="802" columnWidth="515" leftMargin="20" rightMargin="20" topMargin="0" bottomMargin="0" uuid="317f8f60-dc7c-414d-9b95-d151eca4f841">
	<property name="ireport.zoom" value="1.6105100000000008"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="FECHA" class="java.util.Date"/>
	<parameter name="CODALM" class="java.lang.String"/>
	<parameter name="UID_ACTIVIDAD" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT CAB.CODALM,  MIN(TIC.ID_TICKET) AS DOCUMENTO_MINIMO, MAX(TIC.ID_TICKET) AS DOCUMENTO_MAXIMO, TIC.ID_TIPO_DOCUMENTO, TIP.DESTIPODOCUMENTO, TIP.ID_CONTADOR
FROM D_CAJA_CAB_TBL CAB
INNER JOIN D_ALMACENES_TBL ALM
	ON (ALM.UID_ACTIVIDAD = CAB.UID_ACTIVIDAD AND ALM.CODALM = CAB.CODALM)
INNER JOIN D_CAJA_DET_TBL DET
	ON (CAB.UID_ACTIVIDAD = DET.UID_ACTIVIDAD AND CAB.UID_DIARIO_CAJA = DET.UID_DIARIO_CAJA)
INNER JOIN D_TICKETS_TBL TIC
	ON (DET.UID_ACTIVIDAD = TIC.UID_ACTIVIDAD AND DET.ID_DOCUMENTO = TIC.UID_TICKET)
INNER JOIN D_TIPOS_DOCUMENTOS_TBL TIP
	ON (TIC.UID_ACTIVIDAD = TIP.UID_ACTIVIDAD AND TIC.ID_TIPO_DOCUMENTO = TIP.ID_TIPO_DOCUMENTO)
WHERE CAB.UID_ACTIVIDAD = $P{UID_ACTIVIDAD} AND CAST(DET.FECHA AS DATE) = $P{FECHA} AND CAB.CODALM = $P{CODALM} AND
DOCUMENTO IS NOT NULL
GROUP BY CAB.CODALM, TIC.ID_TIPO_DOCUMENTO, TIP.DESTIPODOCUMENTO, TIP.ID_CONTADOR
ORDER BY CAB.CODALM ASC]]>
	</queryString>
	<field name="CODALM" class="java.lang.String"/>
	<field name="DOCUMENTO_MINIMO" class="java.math.BigDecimal"/>
	<field name="DOCUMENTO_MAXIMO" class="java.math.BigDecimal"/>
	<field name="ID_TIPO_DOCUMENTO" class="java.math.BigDecimal"/>
