	<queryString>
		<![CDATA[SELECT CAST(ALBARANCAB.FECHA AS DATE) AS FECHA_DEVOLUCION,
  ALBARANCAB.HORA AS HORA_DEVOLUCION,
  USUARIO.DESUSUARIO AS CAJERO,
  ALBARANDET.CODART,
  ALBARANDET.DESART AS ARTICULO_DEVUELTO,
  ALBARANDET.CANTIDAD AS CANTIDAD_DEVUELTA,
  XMOTIVOS.DESCRIPCION AS MOTIVO_DEVOLUCION,
  ALBARANDET.IMPORTE_TOTAL AS IMPORTE_DEVOLUCION,
  MEDIOSPAGO.DESMEDPAG_VEN AS METODO_PAGO,
  ALMACEN.CODALM,
  ALMACEN.DESALM,
  ALBARANCAB.COD_ALBARAN AS REFERENCIA,
  ALBARANCAB.COD_ALBARAN_ORIGEN AS REFERENCIA_VENTA,
  AUDITORIA.DES_USUARIO_SUPERVISOR AS SUPERVISOR,
  AUDITORIA.PROCEDE AS VALIDADO,
  AUDITORIA.FECHA AS FECHA_VALIDACION
FROM
  X_AUDITORIAS_TBL AUDITORIA INNER JOIN
  D_CLIE_ALBARANES_CAB_TBL ALBARANCAB ON (ALBARANCAB.UID_ACTIVIDAD = AUDITORIA.UID_ACTIVIDAD AND
                                          ALBARANCAB.UID_TICKET    = AUDITORIA.UID_TICKET_VENTA) INNER JOIN
  D_CLIE_ALBARANES_DET_TBL ALBARANDET ON (ALBARANDET.UID_ACTIVIDAD   = ALBARANCAB.UID_ACTIVIDAD AND
                                          ALBARANDET.ID_CLIE_ALBARAN = ALBARANCAB.ID_CLIE_ALBARAN) INNER JOIN
  D_CLIE_ALBARANES_PAG_TBL ALBARANPAG ON (ALBARANDET.UID_ACTIVIDAD   = ALBARANPAG.UID_ACTIVIDAD AND
                                          ALBARANDET.ID_CLIE_ALBARAN = ALBARANPAG.ID_CLIE_ALBARAN) INNER JOIN
  D_TICKETS_TBL TICKET ON (TICKET.UID_ACTIVIDAD = ALBARANCAB.UID_ACTIVIDAD AND
                           TICKET.UID_TICKET    = ALBARANCAB.UID_TICKET) INNER JOIN
  D_MEDIOS_PAGO_VEN_TBL MEDIOSPAGO ON (MEDIOSPAGO.UID_ACTIVIDAD = ALBARANCAB.UID_ACTIVIDAD AND
                                       MEDIOSPAGO.ID_MEDPAG_VEN = ALBARANPAG.ID_MEDPAG_VEN) INNER JOIN
  D_ALMACENES_TBL ALMACEN ON (ALMACEN.UID_ACTIVIDAD = ALBARANCAB.UID_ACTIVIDAD AND
                              ALMACEN.CODALM        = ALBARANCAB.CODALM) LEFT JOIN
  D_USUARIOS_TBL USUARIO ON (USUARIO.DESUSUARIO    = ALBARANCAB.USUARIO AND
                             USUARIO.UID_INSTANCIA = AUDITORIA.UID_INSTANCIA) LEFT JOIN
  X_CLIE_ALBARANES_DET_TBL XALBARANDET ON (XALBARANDET.UID_ACTIVIDAD   = ALBARANCAB.UID_ACTIVIDAD AND
                                           XALBARANDET.ID_CLIE_ALBARAN = ALBARANCAB.ID_CLIE_ALBARAN AND
                                           XALBARANDET.LINEA           = ALBARANDET.LINEA) LEFT JOIN
  X_MOTIVOS_TBL XMOTIVOS ON (XMOTIVOS.UID_ACTIVIDAD = ALBARANCAB.UID_ACTIVIDAD AND
                             XMOTIVOS.CODIGO        = XALBARANDET.CODIGO AND
                             XMOTIVOS.CODIGO_TIPO   = '1')

WHERE (ALBARANCAB.FECHA >= $P{FECHA_DESDE} AND ALBARANCAB.FECHA <= $P{FECHA_HASTA})
  AND ($P{COD_TIENDA} IS NULL OR $P{COD_TIENDA} IS NOT NULL AND ALMACEN.CODALM = $P{COD_TIENDA})
  AND ($P{COD_ARTICULO} IS NULL OR $P{COD_ARTICULO} IS NOT NULL AND ALBARANDET.CODART = $P{COD_ARTICULO})
  AND ($P{REFERENCIA_FACTURA} IS NULL OR $P{REFERENCIA_FACTURA} IS NOT NULL AND ALBARANCAB.COD_ALBARAN = $P{REFERENCIA_FACTURA})
  AND AUDITORIA.TIPO_EVENTO LIKE '%DEVOLUCION%'
ORDER BY
  ALMACEN.CODALM DESC,
  ALBARANCAB.FECHA DESC,
  ALBARANCAB.HORA DESC,
  ALBARANCAB.ID_CLIE_ALBARAN DESC,
  ALBARANDET.LINEA DESC,
  ALBARANDET.CODART DESC
   ]]>
	</queryString>
