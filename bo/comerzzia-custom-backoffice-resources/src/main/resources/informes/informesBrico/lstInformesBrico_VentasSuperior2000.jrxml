	<queryString>
		<![CDATA[WITH CLIENTES_PREFILTRADOS AS (
  SELECT codcli
  FROM d_clie_albaranes_cab_tbl cab
  WHERE cab.periodo = $P{PERIODO}
  GROUP BY codcli
  HAVING SUM(cab.total) >= 2000
),
CLIENTES_EFECTIVO_2000 AS (
  SELECT
    cab.codcli,
    COALESCE(
      cab.descli,
      CONCAT_WS(' ', fid_t.nombre, fid_t.apellidos),
      CONCAT_WS(' ', fid_d.nombre, fid_d.apellidos)
    ) AS nombre,
    COALESCE(fid_t.documento, fid_d.documento, cab.cif) AS documento,
    COALESCE(email_t.valor, email_d.valor, cli.email) AS email,
    COALESCE(tel_t.valor, tel_d.valor, cli.telefono1) AS telefono
  FROM d_clie_albaranes_cab_tbl cab
  JOIN CLIENTES_PREFILTRADOS cp ON cp.codcli = cab.codcli
  JOIN d_clientes_tbl cli
    ON cli.uid_actividad = cab.uid_actividad
   AND cli.codcli = cab.codcli
  LEFT JOIN f_tarjetas_tbl tar
    ON tar.uid_instancia = cab.uid_actividad
   AND tar.numero_tarjeta = cab.tarjeta_fidelizacion
  LEFT JOIN f_fidelizados_tbl fid_t
    ON fid_t.uid_instancia = tar.uid_instancia
   AND fid_t.id_fidelizado = tar.id_fidelizado
  LEFT JOIN f_fidelizados_contacto_tbl email_t
    ON email_t.uid_instancia = cab.uid_actividad
   AND email_t.id_fidelizado = fid_t.id_fidelizado
   AND email_t.codtipocon = 'EMAIL'
  LEFT JOIN f_fidelizados_contacto_tbl tel_t
    ON tel_t.uid_instancia = cab.uid_actividad
   AND tel_t.id_fidelizado = fid_t.id_fidelizado
   AND tel_t.codtipocon IN ('MOVIL', 'TELEFONO1')
  LEFT JOIN f_fidelizados_tbl fid_d
    ON fid_d.uid_instancia = cab.uid_actividad
   AND fid_d.documento = cab.cif
  LEFT JOIN f_fidelizados_contacto_tbl email_d
    ON email_d.uid_instancia = cab.uid_actividad
   AND email_d.id_fidelizado = fid_d.id_fidelizado
   AND email_d.codtipocon = 'EMAIL'
  LEFT JOIN f_fidelizados_contacto_tbl tel_d
    ON tel_d.uid_instancia = cab.uid_actividad
   AND tel_d.id_fidelizado = fid_d.id_fidelizado
   AND tel_d.codtipocon IN ('MOVIL', 'TELEFONO1')
  JOIN d_clie_albaranes_pag_tbl pag
    ON pag.uid_actividad = cab.uid_actividad
   AND pag.id_clie_albaran = cab.id_clie_albaran
   AND pag.id_medpag_ven = 2
  WHERE cab.periodo = $P{PERIODO}
    AND cab.codcli <> cab.codalm
  GROUP BY cab.codcli, nombre, documento, email, telefono
  HAVING SUM(pag.importe) >= 2000
)
SELECT
  cab.codcli,
  pag.importe AS importe,
  cab.referencia_cliente AS n_transaccion,
  ce2k.nombre,
  ce2k.documento,
  ce2k.email,
  ce2k.telefono
FROM d_clie_albaranes_pag_tbl pag
JOIN d_clie_albaranes_cab_tbl cab
  ON cab.uid_actividad = pag.uid_actividad
 AND cab.id_clie_albaran = pag.id_clie_albaran
JOIN CLIENTES_EFECTIVO_2000 ce2k
  ON ce2k.codcli = cab.codcli
WHERE pag.id_medpag_ven = 2
  AND cab.periodo = $P{PERIODO}
  AND cab.codcli <> cab.codalm
  AND (
    $P{DOCUMENTO} IS NULL OR
    $P{DOCUMENTO} = ce2k.documento
  )
ORDER BY cab.codcli, cab.periodo;]]>
	</queryString>
