	<queryString>
		<![CDATA[SELECT DISTINCT XAUD.UID_TICKET_AUDITORIA,
  ART.CODART,
  ART.DESART,
  XAUD.DES_USUARIO_SUPERVISOR,
  CAB.FECHA,
  CAB.REFERENCIA_CLIENTE AS N_FACTURA_DEV,
  XAUD.PRECIO_APLICADO,
  XAUD.PRECIO_ORIGINAL,
  MOT.DESCRIPCION,
  FAM.CODFAM,
  USU.ID_USUARIO,
  CAB.CODALM

FROM
  D_CLIE_ALBARANES_CAB_TBL CAB INNER JOIN
  D_CLIE_ALBARANES_DET_TBL DET ON (DET.UID_ACTIVIDAD   = CAB.UID_ACTIVIDAD AND
                                   DET.ID_CLIE_ALBARAN = CAB.ID_CLIE_ALBARAN) LEFT JOIN
  X_CLIE_ALBARANES_DET_TBL XDET ON (XDET.UID_ACTIVIDAD   = DET.UID_ACTIVIDAD AND
                                    XDET.ID_CLIE_ALBARAN = DET.ID_CLIE_ALBARAN AND
                                    XDET.LINEA           = DET.LINEA) INNER JOIN
  D_ARTICULOS_TBL ART ON (ART.UID_ACTIVIDAD = DET.UID_ACTIVIDAD AND
                          ART.CODART        = DET.CODART) LEFT JOIN
  D_FAMILIAS_TBL FAM ON (FAM.UID_ACTIVIDAD = CAB.UID_ACTIVIDAD AND
                         FAM.CODFAM        = ART.CODFAM) LEFT JOIN
  X_MOTIVOS_TBL MOT ON (MOT.UID_ACTIVIDAD = XDET.UID_ACTIVIDAD AND
                        MOT.CODIGO        = XDET.CODIGO) INNER JOIN
   X_AUDITORIAS_TBL XAUD ON (XAUD.UID_ACTIVIDAD    = ART.UID_ACTIVIDAD AND
                            XAUD.CODART           = ART.CODART AND
                            XAUD.UID_TICKET_VENTA = CAB.UID_TICKET AND
                             (XAUD.LINEA_REFERENCIA IS NULL OR (XAUD.LINEA_REFERENCIA IS NOT NULL AND DET.linea = XAUD.LINEA_REFERENCIA))) INNER JOIN
  D_USUARIOS_TBL USU ON (USU.UID_INSTANCIA = XAUD.UID_INSTANCIA AND
                         USU.USUARIO       = CAB.USUARIO)

WHERE
  XAUD.TIPO_EVENTO = 'CAMBIO_PRECIO' AND
  (CAB.FECHA BETWEEN $P{FECHA_DESDE} AND $P{FECHA_HASTA}) AND
  ($P{UID_ACTIVIDAD} IS NULL OR ($P{UID_ACTIVIDAD} IS NOT NULL AND CAB.UID_ACTIVIDAD = $P{UID_ACTIVIDAD})) AND
  ($P{IDUSUARIO_DESDE} IS NULL OR ($P{IDUSUARIO_DESDE} IS NOT NULL AND USU.ID_USUARIO >= $P{IDUSUARIO_DESDE})) AND
  ($P{IDUSUARIO_HASTA} IS NULL OR ($P{IDUSUARIO_HASTA} IS NOT NULL AND USU.ID_USUARIO <= $P{IDUSUARIO_HASTA})) AND
  ($P{CODART_DESDE} IS NULL OR ($P{CODART_DESDE} IS NOT NULL AND ART.CODART >= $P{CODART_DESDE})) AND
  ($P{CODART_HASTA} IS NULL OR ($P{CODART_HASTA} IS NOT NULL AND ART.CODART <= $P{CODART_HASTA})) AND
  ($P{CODFAM_DESDE} IS NULL OR ($P{CODFAM_DESDE} IS NOT NULL AND FAM.CODFAM >= $P{CODFAM_DESDE})) AND
  ($P{CODFAM_HASTA} IS NULL OR ($P{CODFAM_HASTA} IS NOT NULL AND FAM.CODFAM <= $P{CODFAM_HASTA})) AND
  ($P{CODMOTIVO_DESDE} IS NULL OR ($P{CODMOTIVO_DESDE} IS NOT NULL AND MOT.DESCRIPCION >= $P{CODMOTIVO_DESDE})) AND
  ($P{CODMOTIVO_HASTA} IS NULL OR ($P{CODMOTIVO_HASTA} IS NOT NULL AND MOT.DESCRIPCION <= $P{CODMOTIVO_HASTA})) AND
  ($P{CODALMACEN_DESDE} IS NULL OR ($P{CODALMACEN_DESDE} IS NOT NULL AND CAB.CODALM >= $P{CODALMACEN_DESDE})) AND
  ($P{CODALMACEN_HASTA} IS NULL OR ($P{CODALMACEN_HASTA} IS NOT NULL AND CAB.CODALM <= $P{CODALMACEN_HASTA}))

ORDER BY CAB.FECHA,CAB.REFERENCIA_CLIENTE ASC]]>
	</queryString>
